From e5dc0248adbea72cc606e945014f58727af98ab0 Mon Sep 17 00:00:00 2001
From: root <root@gitlab>
Date: Wed, 23 Sep 2015 22:07:35 +0000
Subject: [PATCH 10/10] Use a properly black text color

---
 app/assets/stylesheets/base/variables.scss     | 2 +-
 app/assets/stylesheets/generic/typography.scss | 2 +-
 2 files changed, 2 insertions(+), 2 deletions(-)

diff --git a/app/assets/stylesheets/base/variables.scss b/app/assets/stylesheets/base/variables.scss
index 857191b..ad1f635 100644
--- a/app/assets/stylesheets/base/variables.scss
+++ b/app/assets/stylesheets/base/variables.scss
@@ -2,7 +2,7 @@ $hover: #FFFAF1;
 $gl-text-color: #54565b;
 $gl-header-color: #4c4e54;
 $gl-link-color: #333c48;
-$md-text-color: #444;
+$md-text-color: #000;
 $md-link-color: #3084bb;
 $nprogress-color: #c0392b;
 $gl-font-size: 14px;
diff --git a/app/assets/stylesheets/generic/typography.scss b/app/assets/stylesheets/generic/typography.scss
index d5f0d86..2857726 100644
--- a/app/assets/stylesheets/generic/typography.scss
+++ b/app/assets/stylesheets/generic/typography.scss
@@ -19,7 +19,7 @@ pre {
   font-family: $monospace_font;
 
   &.dark {
-    background: #333;
+    background: #000;
     color: $background-color;
   }
 
